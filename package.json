{
  "name": "karma_api",
  "version": "1.0.0",
  "node": "20.14.0",
  "engines": {
    "node": "20.x"
  },
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/main.js",
    "start:dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/main.js\"",
    "migration:generate": "sequelize-cli migration:generate --name ",
    "migration:run": "sequelize-cli db:migrate && npm run seed:run",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "seed:generate": "sequelize-cli seed:generate --name ",
    "seed:run": "sequelize-cli db:seed:all",
    "seed:revert": "sequelize-cli db:seed:undo",
    "copy-files": "copyfiles -u 1 src/**/*.xlsx dist",
    "postbuild": "npm run copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-personal:migbau1/karma_api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bwip-js": "^4.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formidable": "^3.5.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mysql2": "^3.6.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.34.0",
    "sharp": "^0.33.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.10.0",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.3.2"
  }
}
